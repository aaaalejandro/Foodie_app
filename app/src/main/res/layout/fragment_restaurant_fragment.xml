<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".restaurant_fragment">

    <!-- TODO: Update blank fragment layout -->
    <ImageView
        android:id="@+id/imgNotFoundCarrito2"
        android:layout_width="238dp"
        android:layout_height="216dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="388dp"
        android:src="@drawable/img_notfound_carrito"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/img_notfound_platos"
        tools:srcCompat="@drawable/img_notfound_platos" />

    <ImageView
        android:id="@+id/imgTextNotFoundCarrito2"
        android:layout_width="342dp"
        android:layout_height="325dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="180dp"
        android:src="@drawable/img_textnotfound_carrito"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/img_notfoundplatos"
        tools:srcCompat="@drawable/img_notfoundplatos" />

    <Button
        android:id="@+id/btnCatPostres"
        android:layout_width="79dp"
        android:layout_height="29dp"
        android:layout_marginStart="274dp"
        android:layout_marginTop="102dp"
        android:background="#00FFFFFF"
        android:text="Postres"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnCatSnacks"
        android:layout_width="68dp"
        android:layout_height="28dp"
        android:layout_marginStart="205dp"
        android:layout_marginTop="102dp"
        android:background="#00FFFFFF"
        android:text="snacks"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnCatBebidas"
        android:layout_width="79dp"
        android:layout_height="29dp"
        android:layout_marginStart="126dp"
        android:layout_marginTop="101dp"
        android:background="#00FFFFFF"
        android:text="Bebidas"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnBuscarRest2"
        android:layout_width="38dp"
        android:layout_height="40dp"
        android:layout_marginStart="324dp"
        android:layout_marginTop="181dp"
        android:background="@drawable/imglupa"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/txtBuscarRest2"
        android:layout_width="342dp"
        android:layout_height="45dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="176dp"
        android:background="@drawable/txtcontrase_a_shape"
        android:ems="10"
        android:hint="¿Qué comida estás buscando?"
        android:inputType="textEmailAddress"
        android:paddingStart="16dp"
        android:paddingEnd="16dp"
        android:selectAllOnFocus="false"
        android:singleLine="false"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="#673AB7"
        android:textColorHint="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imgUser3"
        android:layout_width="54dp"
        android:layout_height="50dp"
        android:layout_marginStart="320dp"
        android:layout_marginTop="28dp"
        android:src="@drawable/user_preview"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/user_preview"
        tools:srcCompat="@drawable/user_preview" />

    <ImageView
        android:id="@+id/imgLogoMenu2"
        android:layout_width="54dp"
        android:layout_height="50dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="29dp"
        android:src="@drawable/logo_transparent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_transparent"
        tools:srcCompat="@drawable/logo_transparent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="345dp"
        android:layout_height="11dp"
        android:layout_marginStart="33dp"
        android:layout_marginTop="150dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/hr" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="347dp"
        android:layout_height="61dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="89dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/fondonav" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="33dp"
        android:layout_marginTop="238dp"
        android:text="Listado de platos"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnCatPlatos"
        android:layout_width="79dp"
        android:layout_height="29dp"
        android:layout_marginStart="47dp"
        android:layout_marginTop="101dp"
        android:background="#00FFFFFF"
        android:text="Platos"
        android:textColor="#F44336"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</FrameLayout>